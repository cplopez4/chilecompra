  <script>
  var mySwiper = new Swiper('.swiper-container',{
    loop:false,
    grabCursor: false,
    paginationClickable: false,
    noSwiping: true,
    moveStartThreshold: 10
  });
  
  $(document).ready(function(){
    $(".back-cont").click(function(){
      mySwiper.swipePrev();
    })
  });
  $(document).ready(function(){
    $(".back-cont-list").click(function(){
      mySwiper.swipePrev();
    })
  });
  $(document).ready(function(){
    $(".back-cont-detail").click(function(){
      mySwiper.swipePrev();
    })
  });
  </script>

 

